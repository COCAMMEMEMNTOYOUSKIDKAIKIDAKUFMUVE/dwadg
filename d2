shared.targetapi = {}

local BleachhackTargetHud = Instance.new("ScreenGui")
BleachhackTargetHud.Name = "BleachhackTargetHud"
BleachhackTargetHud.Parent = game.CoreGui
local TargetHud = Instance.new("Frame")
TargetHud.Name = "TargetHud"
TargetHud.Parent = BleachhackTargetHud
TargetHud.BackgroundColor3 = Color3.fromRGB(93, 13, 13)
TargetHud.BackgroundTransparency = 1
TargetHud.Position = UDim2.new(0.48568362, 0, 0.476928383, 0)
TargetHud.Size = UDim2.new(0, 251, 0, 82)
TargetHud.Visible = false
local targetinfoscale = Instance.new("UIScale")
targetinfoscale.Parent = TargetHud
TargetHud:GetPropertyChangedSignal("Size"):connect(function()
    local oldxoffset = TargetHud.Size.X.Offset
    local oldyoffset = TargetHud.Size.Y.Offset
    task.wait()
    targetinfoscale.Scale = oldxoffset / (shared.currentXoffset or 307)
    targetinfoscale.Scale = oldyoffset / (shared.currentYoffset or 104)
end)
local MainFrame = Instance.new("Frame")
MainFrame.Name = "MainFrame"
MainFrame.Parent = TargetHud
MainFrame.BackgroundColor3 = Color3.fromRGB(4, 4, 4)
MainFrame.BackgroundTransparency = 0.300
MainFrame.BorderColor3 = Color3.fromRGB(150, 5, 5)
MainFrame.BorderSizePixel = 4
MainFrame.Position = UDim2.new(0, 4, 0, 0)
MainFrame.Size = UDim2.new(1, -8, 0.944745004, 0)
local playericon = Instance.new("ImageLabel")
